{
    "dc": {
        "process_id": "load_collection",
        "description": "Loading the data; The order of the specified bands is important for the following reduce operation.",
        "parameters": {
            "id": "S2MSI2A",
            "spatial_extent": {
                "west": 16.1,
                "east": 16.6,
                "north": 48.6,
                "south": 47.2
            },
            "temporal_extent": ["2018-05-01", "2018-05-04"],
            "bands": ["B8"]
        }
    },
    "mintime": {
        "process_id": "reduce",
        "description": "Compute a min time composite by reducing the temporal dimension",
        "parameters": {
            "data": {"from_node": "dc"},
            "dimension": "temporal",
            "reducer": {
                "callback": {
                    "min": {
                        "process_id": "median",
                        "parameters": {
                            "data": {"from_argument": "data"}
                        },
                        "result": true
                    }
                }
            }
        }
    },
    "aggreg1": {
        "process_id": "aggregate_polygon",
        "description": "aggregate polygon using mean value",
        "parameters": {
            "data": {"from_node": "mintime"},
            "polygons": {
                "type": "Polygon",
                "coordinates": [
                    [
                        [16.138916,48.320647],
                        [16.524124,48.320647],
                        [16.524124,48.1386],
                        [16.138916,48.1386],
                        [16.138916,48.320647]
                    ]
                ]
            },
            "reducer": {
                "callback": {
                    "mean1": {
                        "process_id": "median",
                        "parameters": {
                            "data": {"from_argument": "data"}
                        },
                        "result": true
                    }
                }
            }
        }
    },
    "save": {
        "process_id": "save_result",
        "parameters": {
            "data": {"from_node": "aggreg1"},
            "format": "JSON"
        },
        "result": true
    }
}
